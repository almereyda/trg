<% layout('./layout') %>


<!--<% if (it.title) { %>
<header>
  <h1 id='title'><%= it.title %></h1>
</header>
<% } %>
-->
<%~ it.body %>

<hr />

<% if (it.pages.length > 0) {%>
<h4>Index</h4>
<ul id="index">
  <% it.pages.forEach(link => { %>
  <% const href = link.slug === "index" ? "/" : `/${link.slug}` %>
  <li>
    <a href="/<%= link.slug %>"><%= link.title %></a>
  </li>
  <% }) %>
</ul>
<% } %>

<hr />

<%~ includeFile('./backlinks', {backlinks: it.backlinks}) %>

