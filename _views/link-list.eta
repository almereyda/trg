<nav>
  <h3><%= it.title %></h3>

  <ul>
    <% it.links.forEach(({ url, title, date, readableDate, isIndexPage }) => { %>
    <li>
      <a href="<%= url %>">
        <% if (isIndexPage) { %>
        <span>❖ <strong><%= title %>/</strong></span>
        <% } else { %>
        <span>◇ <%= title %></span>
        <% } %>

        <% if (date && readableDate) { %>
        <time datetime="<%= date %>"><%= readableDate %></time>
        <% } %>
      </a>
    </li>
    <% }) %>
  </ul>
</nav>
