<nav aria-label="Breadcrumb">
  <ol>
    <% if (it.breadcrumbs.length > 0) { %>
      <% it.breadcrumbs.forEach((crumb, index) => { %>
        <% if (!crumb.current & crumb.url !== "") { %>
          <li class="breadcrumb-item"> <a href="<%= crumb.url %>"><%= crumb.slug %></a></li>
        <% } else { %>
          <li class="breadcrumb-item breadcrumb-item-selected"> <span><%= crumb.slug %></span></li>
        <% } %>
      <% }) %>
    <% } %>
  </ol>
</nav>
