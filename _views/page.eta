<% layout('./layout') %>

<%~ includeFile('./meta-header', {breadcrumbs: it.breadcrumbs, readableDate: it.readableDate }) %>

<article class="markdown-body">
  <%~ it.body %>
</article>

<% if (it.indexLinks?.length > 0) { %>
<%~ includeFile('./link-list', {links: it.indexLinks, title: "Index"}) %>
<% } %>

<% if (it.backLinks?.length > 0) { %>
<%~ includeFile('./link-list', {links: it.backLinks, title: "Backlinks"}) %>
<% } %>
