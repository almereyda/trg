{
  "tasks": {
    "build": "deno run -A --unstable main.ts --input docs --local",
    "serve": "deno run -A --unstable main.ts --input docs --local  --serve",
    "dev": "deno run -A --unstable --watch=assets/,views/ main.ts --input docs --local --serve",
    "clean": "rm -rf _site"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns"
    ],
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment"
  }
}
